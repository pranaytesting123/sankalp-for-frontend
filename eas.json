{
  "cli": {
    "version": ">= 16.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android" :{
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://sankalp-deploy-1.onrender.com"
      }
    },
    "production": {
      "android" :{
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://sankalp-deploy-1.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
